{
  "extends": ["airbnb", "eslint:recommended", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier", "react"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "env": {
    "node": true,
    "jest": true,
    "browser": true
  }
}
